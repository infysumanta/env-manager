{
  "name": "sumantablog/env-manager",
  "description": "Env Manager for Laravel-superadmin",
  "type": "library",
  "keywords": ["laravel-superadmin", "extension"],
  "homepage": "https://github.com/sumantablog/env-manager",
  "license": "MIT",
  "authors": [{
    "name": "Sumanta Kabiraj",
    "email": "sumantablog@gmail.com"
  }],
  "require": {
    "php": ">=7.0.0",
    "sumantablog/laravel-superadmin": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Sumantablog\\EnvManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sumantablog\\EnvManager\\EnvManagerServiceProvider"
      ]

    }
  }
}